#include <stdio.h>
#include <stdlib.h>

void division(){
    float a, b, resultado;
    
    printf("Ingrese el numero 1: ");
    scanf("%f", &a);
    
    printf("Ingrese el numero 2: ");
    scanf("%f", &b);

    if(b == 0){
        printf("el dividendo no puede ser cero");
    }else{
        resultado = a / b;
    }


    

     printf("El resultado es: %f\n", resultado);  


}
