#include <stdio.h>
#include <stdlib.h>

void division()
{
    int a, b , resultado;
    
    printf("Ingrese el numero 1: ");
    scanf("%d", &a);
    
    printf("Ingrese el numero 2: ");
    scanf("%d", &b);
        
    resultado = a / b;
    
    printf("El resultado es: %d", resultado);       

}
