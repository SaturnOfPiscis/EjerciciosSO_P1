#include <stdio.h>
#include <stdlib.h>

void division()
{
    int a, b;
    float resultado;
    
    printf("Ingrese el numero 1: ");
    scanf("%i", &a);
    
    printf("Ingrese el numero 2: ");
    scanf("%i", &b);
        
    resultado = a / b;
    
    printf("El resultado es: %f", resultado);       

}
